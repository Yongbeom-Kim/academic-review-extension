{
    "name": "Vite/React/TailwindCSS Plugin",
    "description": "A Cross Borwser Template for Firefox",
    "version": "1.0.0",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "src/popup.html",
      "default_title": "Open the popup"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["assets/background/script.js"],
        "run_at": "document_end"
      }
    ]
  }
  