{
    "name": "Vite/React/TailwindCSS Plugin",
    "description": "A Cross Borwser Template for Firefox",
    "version": "1.0.0",
    "manifest_version": 2,
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["assets/content_script/inject_component.js"]
      }
    ]
  }
  